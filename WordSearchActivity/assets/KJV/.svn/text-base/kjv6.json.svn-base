      "book_id": 23
    }
  }, 
  {
    "pk": 17870, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 10, 
      "text": "And the rest of the trees of his forest shall be few, that a child may write them.", 
      "verse_id": 19, 
      "book_id": 23
    }
  }, 
  {
    "pk": 17871, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 10, 
