    }
  }, 
  {
    "pk": 23011, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 9, 
      "text": "As for thee also, by the blood of thy covenant I have sent forth thy prisoners out of the pit wherein is no water.", 
