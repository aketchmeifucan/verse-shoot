    }
  }, 
  {
    "pk": 20856, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 18, 
      "text": "And hath not eaten upon the mountains, neither hath lifted up his eyes to the idols of the house of Israel, neither hath defiled his neighbour's wife, neither hath come near to a menstruous woman,", 
