  }, 
  {
    "pk": 23012, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 9, 
      "text": "Turn you to the strong hold, ye prisoners of hope: even to day do I declare that I will render double unto thee;", 
