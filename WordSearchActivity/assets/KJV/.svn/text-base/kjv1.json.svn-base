      "book_id": 2
    }
  }, 
  {
    "pk": 1774, 
    "model": "bibletext.kjv", 
    "fields": {
      "chapter_id": 9, 
      "text": "And the flax and the barley was smitten: for the barley was in the ear, and the flax was bolled.", 
      "verse_id": 31, 
      "book_id": 2
    }
  }, 
  {
    "pk": 1775, 
    "model": "bibletext.kjv", 
    "fields": {
